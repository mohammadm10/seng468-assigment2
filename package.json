{
  "name": "express-server-example",
  "version": "1.0.0",
  "description": "A simple ExpressJS server in TypeScript with nodemon",
  "main": "dist/index.js",
  "scripts": {
    "start": "npx ts-node src/index.ts",
    "dev": "nodemon --watch src -e ts --exec \"ts-node\" src/index.ts",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-jwt": "^8.4.0",
    "faker": "^5.5.3",
    "jwks-rsa": "^3.0.1",
    "luxon": "^3.3.0",
    "mongodb": "^5.1.0",
    "nginx": "^1.0.8",
    "redis": "^4.6.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/express": "^4.17.17",
    "@types/faker": "^6.6.9",
    "@types/redis": "^4.0.11",
    "nodemon": "^2.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.0.0"
  }
}
